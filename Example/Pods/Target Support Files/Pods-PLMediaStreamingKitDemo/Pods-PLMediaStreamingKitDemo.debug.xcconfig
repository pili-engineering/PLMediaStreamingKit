OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/HappyDNS" -isystem "${PODS_ROOT}/Headers/Public/PLMediaStreamingKit" -isystem "${PODS_ROOT}/Headers/Public/pili-librtmp"
OTHER_LDFLAGS = $(inherited) -ObjC -l"HappyDNS" -l"PLMediaStreamingKit" -l"c++" -l"pili-librtmp" -l"resolv" -l"z" -framework "AVFoundation" -framework "AudioToolbox" -framework "CFNetwork" -framework "CoreGraphics" -framework "CoreMedia" -framework "UIKit" -framework "VideoToolbox"
PODS_ROOT = ${SRCROOT}/Pods